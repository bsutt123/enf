<div class="container">
  <%= semantic_form_for cabin do |f| %>
    <%= f.inputs do %>
      <%= f.input :number, :include_blank => false %>
      <%= f.input :session, :member_label => :identifier, :include_blank => false  %>
    <% end %>
    <%= f.actions do %>
      <% f.action :submit %>
    <% end %>
  <% end %>
