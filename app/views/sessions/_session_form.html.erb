<div class="container">
  <%= semantic_form_for session do |f| %>
    <%= f.inputs do %>
      <%= f.input :number %>
      <%= f.input :start %>
      <%= f.input :finish %>
    <% end %>
    <%= f.actions do %>
      <%= f.action :submit, :as => :button %>
      <%= f.action :cancel, :as => :link %>
    <% end %>
  <% end %>
</div>
